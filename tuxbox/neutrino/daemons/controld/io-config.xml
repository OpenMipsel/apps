<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE io-config SYSTEM "io-config.dtd">
<io-config>
  <vendor name="nokia">
    <operating-mode mode="dvb">
      <tv-format format="cvbs">
        <v1 value="1"/>
        <vcr-format format="cvbs">
          <v2 value="1"/>
        </vcr-format>
        <vcr-format format="svideo">
          <v2 value="2"/>
        </vcr-format>
	<v3 value="0"/>	<!-- 2 and 4 are useless, unfortunatelly -->
      </tv-format>
      <tv-format format="rgb">
        <v1 value="5"/>
        <vcr-format format="cvbs">
          <v2 value="1"/>
        </vcr-format>
        <vcr-format format="svideo">
          <v2 value="7"/>
        </vcr-format>
	<v3 value="0"/>
