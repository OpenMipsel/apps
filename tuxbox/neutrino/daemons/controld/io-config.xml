          <v2 value="1"/>
        </vcr-format>
        <vcr-format format="svideo">
          <v2 value="2"/>
        </vcr-format>
	<v3 value="0"/>
      </tv-format>
      <tv-format format="yuv_vbs">
        <v1 value="5"/>
        <vcr-format format="cvbs">
